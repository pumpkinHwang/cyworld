        "start": "craco start --max-old-space-size=8192",
        "dev": "craco start --config craco.config.dev.js --max-old-space-size=8192 --host 0.0.0.0",
        "local-prod": "REACT_APP_ENV=production craco start",
        "build": "craco build",
        "deploy": "aws s3 sync build/ s3://ushop-kr.unicity.com --delete --acl public-read",
        "codebuild": "craco --max-old-space-size=13824 build",
        "test": "craco test --env=jsdom",
        "gc": "git rm -r --cached . && git add .",
        "cc": "npm cache clean --force",
        "vc": "npm cache verify --force",
        "is": "npm install && npm start",
        "purge": "rm -rf node_modules/ && rm package-lock.json",
        "pis": "npm run purge && npm run is",
        "pi": "npm run purge && npm install",
        "storybook": "start-storybook -p 6006 -s public --no-dll",
        "build-storybook": "build-storybook -s public --no-dll"
